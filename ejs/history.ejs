<h3 class="text-primary"> productos </h3><br>
<% if (produc.length> 0) { %><div class="table-responsive">
        <table class="table table-dark">
            <tr class="text-warning">
                <th class="font-weight-bold">Nombre</th>
                <th class="font-weight-bold">precio</th>
                <th class="font-weight-bold">imagen</th>
            </tr>
            <% produc.forEach(produc=> { %><tr>
                    <td>
                        <%=produc.name%>
                    </td>
                    <td>
                        <%=produc.price%>
                    </td>
                    <td>
                        <%=produc.image%>
                    </td>
                </tr>
            <% }); %>
        </table>
    </div>
<% } else{ %>
<h3 class="alert alert-danger">no se encontraron datos</h1>
<% } %>